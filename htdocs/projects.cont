<h1 class="mainTitle">Project Gallery</h1>
<section id="projectGallery" class="content">

{% for project_key in meta.projects.keys() if meta.projects[project_key].show %}
{% set project = meta.projects[project_key] %}
<div class="projectBox">
     <img src="{{ project.featured_photo_path }}">
     <h2>{{ project.title }}</h2>
     <p class="summary">{{ project.summary }}</p>
     <p class="more"><a href="{{ project.path }}">more ...</a></p>
</div>
{% endfor %}
</section>
